<view class="container">
  <view class="title">游戏选关</view>
  <view class="levelBox">
    <view class="box" wx:for="{{levels}}" wx:key="levels{{index}}" bind:tap="chooseLevel" 
    data-level="{{index}}">
      <image src="/images/{{item}}" mode=""/>
      <text>第{{index+1}}关</text>
    </view>
  </view>
</view>